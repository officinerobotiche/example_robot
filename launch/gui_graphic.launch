<launch>
    <arg name="name_host" default="localhost" />
    <!-- <env name="ROS_MASTER_URI" value="http://ubuntu.local:11311" /> -->

    <rosparam command="load" file="$(find ros_example_robot)/example_robot.yaml" ns="robot" />
    <param name="robot_description" textfile="$(find ros_example_robot)/example_robot.urdf" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
    <node pkg="rqt_rviz" type="rqt_rviz" name="rqt_rviz" args="-d $(find ros_example_robot)/example_robot.rviz"/>
</launch>
